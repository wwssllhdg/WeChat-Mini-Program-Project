<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.eight.evaluation.mapper.QuestionChoiceMapper">

    <resultMap id="BaseResultMap" type="com.eight.evaluation.entity.QuestionChoice">
            <id property="questionId" column="question_id" jdbcType="INTEGER"/>
            <result property="questionName" column="question_name" jdbcType="VARCHAR"/>
            <result property="questionChoiceA" column="question_choice_a" jdbcType="VARCHAR"/>
            <result property="questionChoiceB" column="question_choice_b" jdbcType="VARCHAR"/>
            <result property="questionChoiceC" column="question_choice_c" jdbcType="VARCHAR"/>
            <result property="questionChoiceD" column="question_choice_d" jdbcType="VARCHAR"/>
            <result property="questionTypeId" column="question_type_id" jdbcType="INTEGER"/>
            <result property="questionAnswer" column="question_answer" jdbcType="VARCHAR"/>
            <result property="questionLevel" column="question_level" jdbcType="INTEGER"/>
    </resultMap>

    <sql id="Base_Column_List">
        question_id,question_name,question_choice_a,
        question_choice_b,question_choice_c,question_choice_d,
        question_type_id,question_answer,question_level
    </sql>
</mapper>
